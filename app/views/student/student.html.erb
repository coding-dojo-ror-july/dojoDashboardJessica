<h1> <%= @student.first_name %> <%= @student.last_name %> </h1>
<h3>Email<%= @student.email %></h3>
<h2>Cohort</h2>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
    </tr>
    <% @cohort.each do |classmate| %>
        <tr>
            <td><%= classmate.first_name %></td>
            <td><%= classmate.last_name %></td>
            <td><%= classmate.email %></td>
        </tr> 
    <% end %>       
</table>
<a href="/">Home</a> | <a href="/dojos/<%= @dojo.id %>/edit">Edit</a>
