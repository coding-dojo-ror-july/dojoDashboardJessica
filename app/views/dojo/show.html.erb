<h1><%= @dojo.branch %></h1><br>
<h2>Address: <%= @dojo.street %></h2><br>
<h2>City: <%= @dojo.city %> State: <%= @dojo.state %> </h2><br>
<h1>Student Roster</h1>
<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Actions</th>
    </tr>
    <% @cohort.each do |student| %>
    <tr>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.email %></td>
        <td><a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>">Show</a> | <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>/edit">Edit</a> | <a href="/dojos/<%= @dojo.id %>/students/<%= student.id %>" data-method="delete">Destroy</a>
    </tr>
    <% end %>
</table>
<a href="/">Home</a> | <a href="/dojos/<%= @dojo.id %>/edit">Edit</a> | <a href="/dojos/<%= @dojo.id %>/students/new">Add Student</a>
