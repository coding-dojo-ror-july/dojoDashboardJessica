<% if flash[:errors] %>
  	<% flash[:errors].each do |error| %>
  		<p><%= error %></p>
    <% end %>
<% end %>
<form action="/dojos/<%= @dojo.id %>" method="POST">
    <input type="hidden" name="_method" value="patch">  
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
  <label for="branch">Branch</label>
  <input type="text" name="branch" value="<%= @dojo.branch %>"></input><br> 
  <label for="street">Street</label>
  <input type="text" name="street" value="<%= @dojo.street %>"></input><br>
  <label for="city">City</label>
  <input type="text" name="city" value="<%= @dojo.city %>"></input><br>
  <label for="state">State</label>
  <input type="text" name="state" value="<%= @dojo.state %>"></input><br>
  <button type="submit">Update</button>
</form>