<h1>Listing Dojos</h1>
<table>
    <tr>
        <th>Coding Dojo Branch</th>
        <th>Street</th>
        <th>City</th>
        <th>State</th>
        <th>Actions</th>
    </tr> 
    <% @dojos.each do |dojo| %>
    <tr>
        <td><%= dojo.branch %></td>
        <td><%= dojo.street %></td>
        <td><%= dojo.city %></td>
        <td><%= dojo.state %></td>
        <td><a href="/dojos/<%= dojo.id %>/show">Show</a> | <a href="/dojos/<%= dojo.id %>/edit">Edit</a> | <a href="/dojos/<%= dojo.id %>/destroy">Destroy</a></td>
    </tr>
    <% end %>    
</table>
<a href="/dojos/new">New Dojo</a>